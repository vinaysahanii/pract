Implement required Testng Annotations for testing a website.
Code:
package testNG;

import org.testng.annotations.Test;
import org.testng.annotations.BeforeClass;

import java.util.concurrent.TimeUnit;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.testng.annotations.AfterClass;
import org.testng.annotations.BeforeSuite;
import org.testng.annotations.AfterSuite;

public class AnnotationsApplication {
	
	WebDriver wd;
	  
  @Test
  public void AddToCart() {
	  wd.findElement(By.id("add-to-cart-sauce-labs-backpack")).click();
	  wd.findElement(By.id("add-to-cart-sauce-labs-bolt-t-shirt")).click();
  }
  
  @Test
  public void CheckOutCart() {
	wd.findElement(By.className("shopping_cart_link")).click();
	wd.manage().timeouts().implicitlyWait(2, TimeUnit.SECONDS);
	wd.findElement(By.id("checkout")).click();
  }
  
  @Test
  public void DeliveryDetails() {
	  wd.findElement(By.id("first-name")).sendKeys("Chriscutan");
	  wd.findElement(By.id("last-name")).sendKeys("Nadar");
	  wd.findElement(By.id("postal-code")).sendKeys("400022");
	  wd.findElement(By.id("continue")).click();
	  wd.manage().timeouts().implicitlyWait(2, TimeUnit.SECONDS);
	  wd.findElement(By.id("finish")).click();
	  wd.manage().timeouts().implicitlyWait(2, TimeUnit.SECONDS);
	  wd.findElement(By.id("back-to-products")).click();
  }
  
  @BeforeClass
  public void beforeClass() {
	  wd.findElement(By.id("user-name")).sendKeys("standard_user");
	  wd.findElement(By.id("password")).sendKeys("secret_sauce");
	  wd.findElement(By.id("login-button")).click();
  }

  @AfterClass
  public void afterClass() {
	  wd.findElement(By.id("react-burger-menu-btn")).click();
	  wd.manage().timeouts().implicitlyWait(2, TimeUnit.SECONDS);
	  wd.findElement(By.id("logout_sidebar_link")).click();
  }

  @BeforeSuite
  public void beforeSuite() {
	System.setProperty("webdriver.chrome.driver", "E:\\selenium-java-3.141.59\\117.0.5938.63\\chromedriver.exe");
	wd = new ChromeDriver();
	wd.get("http://www.saucedemo.com");
  }

  @AfterSuite
  public void afterSuite() {
	  wd.close();
  }

}
